:57,"column":8}},{"generated":{"line":112,"column":0},"source":"src/components/app.js","original":{"line":66,"column":5}},{"name":"React","generated":{"line":116,"column":2},"source":"src/components/app.js","original":{"line":6,"column":18}},{"name":"Component","generated":{"line":116,"column":17},"source":"src/components/app.js","original":{"line":6,"column":24}}],"sources":{"src/components/app.js":"import React from \"react\";\r\nimport Timer from \"timer\";\r\nimport Start from \"start\";\r\nimport Reset from \"reset\";\r\n\r\nclass App extends React.Component {\r\n    constructor() {\r\n        super();\r\n        this.state = {\r\n            timer: 1500,\r\n            timerString: \"25:00\",\r\n            timerSet: 1500,\r\n            buttonLabel: \"Start Timer\",\r\n        };\r\n    }\r\n\r\n    adjustMinutes(num) {\r\n        let newTimer = this.state.timer + num;\r\n        newTimer = (newTimer < 0)? 0 : newTimer;\r\n        this.setState({\r\n            timer: newTimer,\r\n            timerString: timeToString(newTimer),\r\n            timerSet: newTimer\r\n        });\r\n    }\r\n\r\n    startTimer() {\r\n        this.state.myTimer = setInterval( () => {\r\n            let newTime = this.state.timer - 1;\r\n            this.setState({\r\n                timer: newTime,\r\n                timerString: timeToString(newTime)\r\n            });\r\n        }, 1000);\r\n        this.setState({\r\n            buttonLabel: \"Stop Timer\"\r\n        });\r\n    }\r\n\r\n    stopTimer() {\r\n        clearInterval(this.state.myTimer);\r\n        this.setState({\r\n            buttonLabel: \"Start Timer\"\r\n        });\r\n    }\r\n\r\n    resetTimer() {\r\n        clearInterval(this.state.myTimer);\r\n        this.setState({\r\n            buttonLabel: \"Start Timer\",\r\n            timer: this.state.timerSet,\r\n            timerString: timeToString(this.state.timerSet)\r\n        });\r\n    }\r\n\r\n    render() {\r\n        return (\r\n            <div className=\"container-fluid text-center\">\r\n                <h1>Set Timer</h1>\r\n                <Timer timer={this.state.timerString} adjustMinutes={this.adjustMinutes.bind(this)}/>\r\n                <Start startTimer={this.startTimer.bind(this)} label={this.state.buttonLabel} stopTimer={this.stopTimer.bind(this)}/>\r\n                <br/>\r\n                <Reset reset={this.resetTimer.bind(this)}/>\r\n            </div>\r\n        );\r\n    }\r\n}\r\n\r\n\r\n"},"lineCount":null}},"error":null,"hash":"e778baa6c2ba782a16ba82fbaabd4701","cacheData":{"env":{}}}